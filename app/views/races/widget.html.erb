<body>
<%coder = HTMLEntities.new%>
<h1><%=coder.encode(@race.name, :named).html_safe%> - <%=I18n.l @race.date%></h1>

<% if @race.races_count>0%>
  <% first = true
  @race.results.order([:race_detail,:rank]).group_by(&:race_detail).each  do |race, results|%>
    <input id="tab_<%=race.parameterize%>" class='tab' type="radio" name="tabs" <%if first%>checked<% first=false; end%>>
    <label for="tab_<%=race.parameterize%>"><%=race%></label>
  <%end%>
<%end%>
<input type="text" id="kapp10-filter" onkeyup="participant_filter()" placeholder="Rechercher un coureur...">


<%
  @race.results.order([:race_detail,:rank]).group_by(&:race_detail).each  do |race, results|
%>
  <section id="content_<%=race.parameterize unless race.nil?%>">
    <table class='results'>
      <thead>
        <tr>
          <th>Class.<br>Scratch</th>
          <th>Nom</th>
          <th>Dossard</th>
          <th>Temps</th>
          <th>Sexe</th>
          <th>Class.<br>Sexe</th>
          <th>Cat&eacute;gorie</th>
          <th>Class.<br>Categ.</th>
        </tr>
      </thead>
      <tbody>
      <%results.each do |result|%>
        <tr class='<%=result.sex%>'  data-search='<%=  I18n.transliterate(result.name).upcase%>'>
          <td class='center'><%=result.rank%></td>
          <td class='left'><%= coder.encode(result.name, :named).html_safe%> </td>
          <td class='center'><%=result.bib%></td>
          <td class='center'><%=result.time%></td>
          <td class='center'><%=result.sex%></td>
          <td class='center'><%=result.sex_rank%></td>
          <td class='center'><%=result.categ%></td>
          <td class='center'><%=result.categ_rank%></td>
        </tr>
      <%end%>
      </tbody>
    </table>
  </section>
<%end%>
<p><em>R&eacute;sultats g&eacute;n&eacute;r&eacute;s le <%=I18n.l @generated_at%>.</em></p>
<p style='text-align: center;'><a style='text-decoration: none; color: black' href='http://kapp10.com'>R&eacute;sultats fournis par <img src='http://results-widget.kapp10.com.s3.amazonaws.com/logo_kapp10.png' style='height: 1em'></a></p>
