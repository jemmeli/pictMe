function participant_filter() {
  var input, filter, tr, i, n;
  input = document.getElementById("kapp10-filter");
  filter = removeDiacritics(input.value).toUpperCase();
  tr = document.getElementsByTagName("tr");
  n = tr.length;
  for (i = 0; i < n ; i++) {
    var search_string;
    search_string = tr[i].getAttribute("data-search");
    if (search_string) {
      bib = tr[i].getElementsByTagName("td")[2].innerHTML;
      if ((search_string.indexOf(filter) > -1) || (bib.indexOf(filter) > -1)){
        tr[i].style.display = "";
      } else {
        tr[i].style.display = "none";
      }
    }
  }
  parentIFrame.size();
}
