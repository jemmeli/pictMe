<style>
  body {
    font-family: "Avenir", "Arial", Helvetica, Arial, "Lucida Grande", sans-serif;
    font-size: 0.8em !important;
    font-weight: 500 !important;
    line-height: 1.8em !important;
  }

  table.results {
    width: 100%;
  }

  h1 {
    padding-top: 1em;
    text-align:left;
    font-size: 1.5em;
  }

  table.results thead {
    background-color: #EEEEEE;
    font-weight: bold;
    line-height: 2em;
    font-size: .9em;
  }

  table.results tbody {
    line-height: 1em;
    font-size: .9em;
  }
  table.results tbody td {
    padding: 5px 5px;
  }

  table.results tbody td.right{
      text-align: right;
  }
  table.results tbody td.left{
      text-align: left;
  }

  table.results tbody td.center{
      text-align: center;
  }

  table.results tbody tr:nth-child(even){
    background-color: #EEEEEE;
  }
  table.results tbody tr.F td {
    font-style: italic;
    color: orange;
  }
</style>

<style>
  section {
    display: none;
    padding: 20px 0 0;
    border-top: 1px solid #ddd;
    overflow-x:auto;
    clear: both;
  }

  input.tab {
    display: none;
  }
  input#kapp10-filter {
    font-weight: 600;
    padding: 10px 25px;
    float: right;
  }

  label {
    display: inline-block;
    margin: 0 0 -1px;
    padding: 10px 25px 5px;
    font-weight: 600;
    text-align: center;
    color: #bbb;
    border: 1px solid transparent;
  }

  label:hover {
    color: #888;
    cursor: pointer;
  }

  input:checked + label {
    color: #555;
    border: 1px solid #ddd;
    border-top: 4px solid orange;
    border-bottom: 1px solid #fff;
  }

    <% if @race.races_count >0 %>
    <%= @race.results.order([:race_detail,:rank]).group_by(&:race_detail).map  { |race, results|
    "#tab_#{race.parameterize}:checked ~ #content_#{race.parameterize}"}.join(',')%>  {
      display: block;
    }
    <%end%>
</style>

